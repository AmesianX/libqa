{{#if spaceMainList}}
    {{#each spaceMainList}}
    <div class="best-qna-item">
        <div>
            <strong><a href="/space/{{space.spaceId}}">{{space.title}}</a></strong>
            <div class="pull-right">
                {{> common/_keywordBadge}}
            </div>
        </div>
        <div class="media">
            <a class="pull-left" href="#">
                <div class="user-profile">
                    <img alt="avatar" class="profile-image" src="/imageView?path={{space.titleImagePath}}/{{space.titleImage}}" />
                </div>
            </a>
            <div class="media-body">{{subString (htmlDelete space.description) 0 100 }}
                <div class="pull-right"><span class="badge">
                    {{wikiCount}}
                </span></div>
            </div>
            <div class="pull-left"><span class="nickname"><i class="fa fa-pencil-square-o"></i> {{space.insertUserNick}}</span></div>
            <div class="pull-right"><i class="fa fa-clock-o"></i> {{formatDate space.insertDate "yyyy-MM-dd hh:MM"}}</div>
        </div>
    </div>
    {{/each}}
{{^}}
    <div class="best-qna-item">
        생성된 공간이 없습니다.
    </div>

{{/if}}
