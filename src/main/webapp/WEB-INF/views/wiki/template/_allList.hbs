{{#each allWiki}}
    <div class="media qna-item">
        <a class="pull-left" href="#">
            <div class="user-profile">
                <img src="/resource/images/avatar.png" class="profile-image" alt="avatar">
            </div>
        </a>
        <div class="row">
            <div class="col-xs-6 col-sm-8 col-md-8">
                <div class="media-body">
                    <div class="media-heading">
                        <span>{{formatDate insertDate "yyyy.MM.dd HH:mm:SS"}}</span>
                        <div class="pull-right">
                            {{#each keywordList}}
                                <span class="label label-primary">{{keywordName}}</span>
                            {{/each}}
                        </div>
                    </div>
                    <div>{{subString (htmlDelete contents) 0 100 }}</div>
                </div>
            </div>
            <div class="col-md-2 my-qna-rating">
                <button class="btn btn-sm btn-default btn-sm-fixed">댓글 {{replyCount}}</button>
                <button class="btn btn-sm btn-default btn-sm-fixed">추천 {{likeCount}}</button>
            </div>
        </div>
    </div>
{{/each}}
<div class="top-buffer">&nbsp;</div>