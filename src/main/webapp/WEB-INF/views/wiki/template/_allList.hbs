{{#each allWiki}}
    <div class="media qna-item">
        <a class="pull-left" href="#">
            <div class="user-profile">
                <img src="{{user.userImage}}" class="profile-image" alt="avatar">
            </div>
        </a>
        <div class="row">
            <div class="col-xs-6 col-sm-8 col-md-8">
                <div class="media-body">
                    <div class="media-heading">
                        <span>{{formatDate wiki.insertDate "yyyy.MM.dd HH:mm:SS"}}</span>
                        <div class="pull-right">
                            {{> common/_keywordBadge}}
                        </div>
                    </div>
                    <div>
                        <a class="form" href="/wiki/{{wiki.wikiId}}">
                            {{{subString (htmlDelete wiki.contents) 0 80 }}}
                        </a>
                    </div>
                </div>
            </div>
            <div class="col-md-2 my-qna-rating">
                <button class="btn btn-sm btn-default btn-sm-fixed">댓글 {{size wiki.wikiReplies }}</button>
                <button class="btn btn-sm btn-default btn-sm-fixed">추천 {{size wiki.wikiLikes }}</button>
            </div>
        </div>
    </div>
{{/each}}
<div class="top-buffer">&nbsp;</div>